<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:ChaosgateKoreanPatch"
             x:Class="ChaosgateKoreanPatch.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
		<Style Selector="TextBox">
			<Setter Property="FontFamily" Value="avares://ChaosgateKoreanPatch/Assets/NanumBarunGothic.ttf#NanumBarunGothic" />
		</Style>
    </Application.Styles>
</Application>
